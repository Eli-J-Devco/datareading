	<select id="getListTriggerFaultCode" resultType="Map" parameterType="String">
		SELECT
			l.id,
			l.id_error,
			l.id_device,
			l.`status`,
			DATE_FORMAT(l.start_date,'%Y-%m-%d %H:%i:%s') AS start_date,
			DATE_FORMAT(NOW(),'%Y-%m-%d %H:%i:%s') AS end_date			
		FROM
			`alert` l
			LEFT JOIN error e ON e.id = l.id_error
			WHERE l.`status` = 1
			AND e.is_delete = 0 AND e.`status` = 1
			AND l.is_delete = 0 AND l.id_device = #{id_device}
			<if test="faultCodeLevel == 1">
				AND l.id_error IN(
				569,570,
				571,572,573,574,575,576,577,578,579,580,
				581,582,583,584,585
				)
			</if>
			
			<if test="faultCodeLevel == 2">
				AND l.id_error IN(
				586,587,588,589,590,
				591,592,593,594,595,596,597,598,599,600,
				601
				)
			</if>
			
			<if test="faultCodeLevel == 3">
				AND l.id_error IN(
				602,603,604,605,606,607,608,609,610,
				611,612,613,614,615,616,617
				)
			</if>
			
			<if test="faultCodeLevel == 4">
				AND l.id_error IN(
				618,619,620,
				621,622,623,624,625,626,627,628,629,630,
				631,632,633
				)
			</if>
			
			<if test="faultCodeLevel == 5">
				AND l.id_error IN(
				634,635,636,637,638,639,640, 
				641,642,643,644,645,646,647,648,649
				)
			</if>
			
			<if test="faultCodeLevel == 6">
				AND l.id_error IN(
				650, 
				651,652,653,654,655,656,657,658,659,660, 
				661,662,663,664,665
				)
			</if>
			
			<if test="faultCodeLevel == 7">
				AND l.id_error IN(
				666,667,668,669,670
				)
			</if>
			
	</select>
	
	<select id="checkAlertWriteCode" resultType="Map">
    	SELECT
				m.Fault_Word1 AS Fault_Word1,
				m.Fault_Word2 AS Fault_Word2,
				m.Fault_Word3 AS Fault_Word3,
				m.Fault_Word4 AS Fault_Word4,
				m.Fault_Word5 AS Fault_Word5,
				m.Fault_Word6 AS Fault_Word6,
				m.Fault_Word7 AS Fault_Word7
				
			FROM
				${datatablename} m
			WHERE
				id_device = #{id_device}
			ORDER BY
				time DESC 
			LIMIT 20;
  	</select>
  	
  	
  	<select id="getLastRow" resultType="com.nwm.api.entities.ModelSatconPvs357InverterEntity">
		SELECT
			dv.*,
			s.enable_alert
		FROM
			${view_tablename} dv 
			LEFT JOIN device d ON d.id = dv.id_device
			LEFT JOIN site s ON s.id = d.id_site
		WHERE
			dv.id_device = #{id_device}
		ORDER BY
			dv.time DESC 
			LIMIT 1
	</select>
  	

