	</select>
	
	
	<select id="getDeviceVirtualMeter" resultType="com.nwm.api.entities.DeviceEntity">
		SELECT
			t.*
		FROM
			device AS t
		WHERE
			t.id_site = #{id_site} AND id_device_group = 81 LIMIT 1;
	</select>
	
	
	<select id="getLastRow" resultType="com.nwm.api.entities.ModelLevitonVirtualMeterEntity">
		SELECT
			dv.*,
			s.enable_alert
		FROM
			${view_tablename} dv 
			LEFT JOIN device d ON d.id = dv.id_device
			LEFT JOIN site s ON s.id = d.id_site
		WHERE
			dv.id_device = #{id_device}
		ORDER BY
			dv.time DESC 
			LIMIT 1
	</select>
	
	
