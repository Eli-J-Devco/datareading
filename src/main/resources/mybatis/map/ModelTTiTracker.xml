	</select>
	
	<select id="checkAlertWriteCode" resultType="Map">
    	SELECT
				m.Mode AS Mode,
				m.SubMode AS SubMode,
				m.MotorFault AS MotorFault,
				m.RemoteInterfaceFault AS RemoteInterfaceFault,
				m.`InclinometerFault` AS `InclinometerFault`
			FROM
				${datatablename} m
			WHERE
				id_device = #{id_device}
			ORDER BY
				time DESC 
			LIMIT 20
