	<select id="checkAlertWriteCode" resultType="Map">
    	SELECT
				m.active_faults1 AS active_faults1,
				m.active_faults2 AS active_faults2,
				m.active_faults3 AS active_faults3,
				m.limits AS limits,
				m.warnings1 AS warnings1,
				m.`status` AS `status`
			FROM
				${datatablename} m
			WHERE
				id_device = #{id_device}
			ORDER BY
				time DESC 
			LIMIT 20;
  	</select>
  	
  	
  	<select id="getListTriggerFaultCode" resultType="Map" parameterType="String">
		SELECT
			l.id,
			l.id_error,
			l.id_device,
			l.`status`,
			DATE_FORMAT(l.start_date,'%Y-%m-%d %H:%i:%s') AS start_date,
			DATE_FORMAT(NOW(),'%Y-%m-%d %H:%i:%s') AS end_date			
		FROM
			`alert` l
			LEFT JOIN error e ON e.id = l.id_error
			WHERE l.`status` = 1
			AND e.is_delete = 0 AND e.`status` = 1
			AND l.is_delete = 0 AND l.id_device = #{id_device}
			<if test="faultCodeLevel == 1">
				AND l.id_error IN(671,672,673,674,675,676,677,678,679,680,681,682,683,684,685,686,687,688,689,690,691,692)
			</if>
			
			<if test="faultCodeLevel == 2">
				AND l.id_error IN(693,694,695,696,697,698,699,700,701,702,703,704,705,706,707,708,709,710,711,712,713,714,715,716,717,718)
			</if>
			
			<if test="faultCodeLevel == 3">
				AND l.id_error IN(719,720,721,722,723,724,725,726,727,728,729,730)
			</if>
			
			<if test="faultCodeLevel == 4">
				AND l.id_error IN(731,732,733,734,735,736,737,738,739,740)
			</if>
			
			<if test="faultCodeLevel == 5">
				AND l.id_error IN(741,742,743,744,745,746,747,748,749,750,751)
			</if>
			
			<if test="faultCodeLevel == 6">
				AND l.id_error IN(752,753,754,755,756,757,758,759)
				
			</if>
			
	</select>
	
	
	<select id="getLastRow" resultType="com.nwm.api.entities.ModelIVTSolaronEXTEntity">
		SELECT
			dv.*,
			s.enable_alert
		FROM
			${view_tablename} dv 
			LEFT JOIN device d ON d.id = dv.id_device
			LEFT JOIN site s ON s.id = d.id_site
		WHERE
			dv.id_device = #{id_device}
			<if test="time != null" >
	        	AND dv.time <![CDATA[<]]> #{time}
	        </if>
		ORDER BY
			dv.time DESC 
			LIMIT 1
	</select>
  	

</mapper> 